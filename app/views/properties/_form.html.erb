<%= content_for :jsIncludes do %>
    <%= javascript_include_tag params[:controller], "data-turbolinks-track" => true %>
<% end %>
<%= form_for(@property) do |f| %>
  <% if @property.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@property.errors.count, "error") %> prohibited this property from being saved:</h2>

      <ul>
      <% @property.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <br />
    <%= f.label :individual, "Contact: " %>
    <%= f.text_field :individual_name %>
    <%= f.hidden_field :individual_id %>
  <br />
    <%= f.label :street_1, "Street 1: " %>
    <%= f.text_field :street_1 %>
  <br />
    <%= f.label :street_2, "Street 2: " %>
    <%= f.text_field :street_2 %>
  <br />
    <%= f.label :city, "City: " %>
    <%= f.text_field :city %>
  <br />
    <%= f.label :state, "State: " %>
    <%= f.text_field :state %>
  <br />
    <%= f.label :zip, "Zip code: " %>
    <%= f.text_field :zip %>
  <br />
    <%= f.label :notes, "Notes" %>
    <%= f.text_area :notes %>
  <br />
    <%= f.submit "Save", class: "submit" %>
<% end %>
