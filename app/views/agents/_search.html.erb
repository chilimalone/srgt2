<%= form_for(Agent.new, url: agents_search_path) do |f| %>
  <%= f.label :fname, "First Name: " %>
  <%= f.text_field :fname %>
<br />
  <%= f.label :lname, "Last Name:" %>
  <%= f.text_field :lname %>
<br />
  <%= f.label :username, "Username: " %>
  <%= f.text_field :username %>
<br />
  <%= f.submit "Search", class: "submit" %>
<% end %>